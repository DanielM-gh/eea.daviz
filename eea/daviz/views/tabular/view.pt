<div ex:role="view"
  ex:viewClass="Tabular"
  ex:cellSpacing="0"
  ex:cellPadding="0"
  ex:tableStyler="DavizTableStyler"
  ex:rowStyler="DavizTableRowStyler"
  tal:define="
    columns python:[col for col in template.columns];
    labels python:template.labels;
    formats python:template.formats;"
  tal:attributes="
    ex:columns python:', '.join(columns);
    ex:columnLabels python:', '.join(labels);
    ex:columnFormats python:', '.join(formats)"
  tal:condition="columns"></div>
