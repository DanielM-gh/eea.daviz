<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plone"
    purge="False">

    <configlet
        title="Daviz Visualization"
        action_id="daviz"
        appId="eea.daviz"
        category="Plone"
        condition_expr=""
        icon_expr="string:${portal_url}/++resource++eea.daviz.images/presentation_icon.png"
        url_expr="string:${portal_url}/@@daviz-settings"
        visible="True"
        i18n:attributes="title">
        <permission>Manage portal</permission>
    </configlet>

</object>